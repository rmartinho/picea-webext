{
  "manifest_version": 2,
  "name": "Picea",
  "version": "1.0.7",

  "description": "Downloads website pages as epub files",

  "icons": {
    "48": "/icons/icon.svg",
    "96": "/icons/icon.svg"
  },

  "content_scripts": [
    {
      "matches": ["https://clarkesworldmagazine.com/*"],
      "js": ["clarkesworld.ts"]
    }
  ]
}
